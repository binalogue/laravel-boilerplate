{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4.1",
    "ext-PDO": "*",
    "ext-Phar": "*",
    "ext-SimpleXML": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-filter": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pcre": "*",
    "ext-posix": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "lib-libxml": "*",
    "lib-pcre": "*",
    "ericlagarda/nova-text-card": "^1.2",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "gladcodes/keygen": "^1.1",
    "gregoriohc/laravel-nova-theme-responsive": "^0.8",
    "guzzlehttp/guzzle": "^7.0",
    "inertiajs/inertia-laravel": "^0.2",
    "laravel-lang/lang": "~7.0",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/nova": "^3.9",
    "laravel/socialite": "^5.0",
    "laravel/telescope": "^4.0",
    "laravel/tinker": "^2.4",
    "maatwebsite/excel": "^3.1",
    "maatwebsite/laravel-nova-excel": "^1.2",
    "optimistdigital/nova-settings": "^2.5",
    "orangehill/iseed": "^2.6",
    "sbine/route-viewer": "^0.0.7",
    "spatie/data-transfer-object": "^2.5",
    "spatie/laravel-flash": "^1.7",
    "spatie/laravel-robots-middleware": "^1.2",
    "spatie/laravel-schemaless-attributes": "^1.8",
    "spatie/laravel-sluggable": "^2.5",
    "tightenco/ziggy": "^0.9",
    "timothyasp/nova-color-field": "^1.0"
  },
  "require-dev": {
    "facade/ignition": "^2.3",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3",
    "sebastian/phpcpd": "^6.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/App/",
      "Domain\\": "app/Domain/",
      "Support\\": "app/Support/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "test": "vendor/bin/phpunit",
    "lint": "vendor/bin/phpcpd app/ --min-lines=5 && vendor/bin/phpcbf --standard=PSR2 --extensions=php app && vendor/bin/phpcs --standard=PSR2 --extensions=php app"
  }
}
