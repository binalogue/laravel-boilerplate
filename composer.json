{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "ext-exif": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "caouecs/laravel-lang": "~5.0",
    "facade/ignition": "^1.16",
    "fideloper/proxy": "^4.3",
    "kg-bot/laravel-localization-to-vue": "^1.6",
    "laravel/framework": "^6.18",
    "laravel/telescope": "^3.2",
    "laravel/tinker": "^2.3",
    "riverskies/laravel-mobile-detect": "^1.3",
    "spatie/laravel-robots-middleware": "^1.2"
  },
  "require-dev": {
    "astrotomic/ignition-stackoverflow": "^1.5",
    "facade/ignition-self-diagnosis": "^1.0",
    "fzaninotto/faker": "^1.9",
    "laravel/ui": "^1.1",
    "mockery/mockery": "^1.2",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.5",
    "sebastian/phpcpd": "^5.0",
    "squizlabs/php_codesniffer": "^3.5",
    "stefanbauer/ignition-laracasts-tab": "^0.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "test": "vendor/bin/phpunit"
  }
}
