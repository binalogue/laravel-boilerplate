{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4.1",
    "ext-PDO": "*",
    "ext-Phar": "*",
    "ext-SimpleXML": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-filter": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pcre": "*",
    "ext-posix": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "lib-libxml": "*",
    "lib-pcre": "*",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "gladcodes/keygen": "^1.1",
    "guzzlehttp/guzzle": "^7.0",
    "inertiajs/inertia-laravel": "^0.2",
    "laravel-lang/lang": "~7.0",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/socialite": "^5.0",
    "laravel/tinker": "^2.4",
    "laravel/ui": "^3.0",
    "orangehill/iseed": "^2.6",
    "spatie/data-transfer-object": "^2.5",
    "spatie/laravel-flash": "^1.7",
    "spatie/laravel-robots-middleware": "^1.2",
    "spatie/laravel-schemaless-attributes": "^1.8",
    "spatie/laravel-sluggable": "^2.5",
    "tightenco/ziggy": "^0.9"
  },
  "require-dev": {
    "facade/ignition": "^2.3",
    "friendsofphp/php-cs-fixer": "^2.16",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3",
    "sebastian/phpcpd": "^6.0"
  },
  "repositories": [],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/App/",
      "Domain\\": "app/Domain/",
      "Support\\": "app/Support/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.docker', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "check": "composer check:phpcsfixer && composer check:phpcpd",
    "check:phpcpd": "vendor/bin/phpcpd --exclude=vendor --exclude=node_modules .",
    "check:phpcsfixer": "vendor/bin/php-cs-fixer fix",
    "test": "vendor/bin/phpunit"
  }
}
